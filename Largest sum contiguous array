//Largest sum Contiguous Sub array 
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cout<<"Enter the size of array ";
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int total=1<<n;
    int sum=a[0];
    for(int j=1;j<total;j++)
    {
        int s=0;
        for(int i=0;i<n;i++)
        {
            if(j&(1<<i))
            {
                s=s+a[i];
            }
        }
        if(s>sum)
        {
            sum=s;
        }
    }
    cout<<"The Largest sum is "<<sum<<endl;
}
