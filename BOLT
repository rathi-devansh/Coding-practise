//CODECHEF WORLD RECORD
#include <iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        float k1,k2,k3,v,d,e,f;
        cin>>k1>>k2>>k3>>v;
        e=k1*k2*k3*v;
        d=100/e;
        f=((d*100)+0.5)/100;
        if(f<9.58)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
}
