//GOLF
#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    long long  N,k,x,s=0;
	    cin>>N>>x>>k;
	    if(x%k==0)
	    {
	        cout<<"YES"<<endl;
	    }
	    else if((N+1-x)%k==0)
	    {
	        cout<<"YES"<<endl;
	    }
	    else
	    {
	     cout<<"NO"<<endl;   
	    }
	    
	}
}

