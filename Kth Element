// Finding the kth  maximum and minimum element
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the Size of Array ";
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        cout<<a[i];
    }
    cout<<endl;
    int k;
    cout<<"Enter the kth element ";
    cin>>k;
    sort(a,a+n);
    for(int i=0;i<n;i++)
    {
        cout<<a[i];
    }
    cout<<endl;
    cout<<"The kth minimum element is "<<a[0+k-1]<<endl;
    cout<<"The kth maximum element is "<<a[n-k]<<endl;
}
